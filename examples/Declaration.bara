// RUN: bara %s -ast-print | FileCheck %s


//===----------------------------------------------------------------------===//
/// simple declaration
//===----------------------------------------------------------------------===//

var a = 1;
// CHECK: var a = 1;


//===----------------------------------------------------------------------===//
/// tuple declaration 
//===----------------------------------------------------------------------===//

var (a, b) = (1, 2);
// CHECK: var (a, b) = (1, 2);


//===----------------------------------------------------------------------===//
/// tuple in tuple declaration
//===----------------------------------------------------------------------===//

var (a, (b, c)) = (1, (2, 3));
// CHECK: var (a, (b, c)) = (1, (2, 3));


//===----------------------------------------------------------------------===//
/// empty tuple declaration 
//===----------------------------------------------------------------------===//

var () = ();
// CHECK: var () = ();

